{
  "manifest_version": 2,
  "name": "Unquirk",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "http://www.mspaintadventures.com/*"
      ],
      "js": ["jquery-3.3.1.min.js","content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon/icon-16.png",
    "default_title": "Unquirk"
  },
  "icons": {
	  "16": "icons/icon-16.png",
	  "48": "icons/icon-48.png",
	  "128": "icons/icon-128.png"
  }
}
